<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PPC Rental</title>
    <link rel="stylesheet" href="https://getbootstrap.com/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cvlocnguyen.000webhostapp.com/carousel.css">
          @Styles.Render("~/Content/css")
          @Scripts.Render("~/bundles/modernizr")></head>
<body>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark " style="font-size:1.5rem">
        <a class="navbar-brand" href="#" style="font-size:2rem">Carousel</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse "  id="navbarCollapse" >
            <ul class="navbar-nav ml-auto" >
                <li class="nav-item active">
                    <a class="nav-link" href="#">Home </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">View List of Agency Project</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Post Project</a>
                </li>
                
            </ul>
            
        </div>
    </nav>
    <div>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - PPC Rental </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
